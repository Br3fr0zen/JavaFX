<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.effect.*?>
<?import javafx.scene.image.*?>
<?import com.jfoenix.controls.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.BorderPane?>

<BorderPane prefHeight="400.0" prefWidth="600.0" styleClass="background" stylesheets="@DarkTheme.css" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ch.makery.address.view.RootLayoutController">
   <top>
      <ToolBar maxHeight="19.0" pickOnBounds="false" prefHeight="19.0" prefWidth="600.0" style="-fx-padding: 0; -fx-background-color: #3F51B5;" BorderPane.alignment="CENTER">
         <items>
            <ImageView fx:id="closeButton" fitHeight="31.0" fitWidth="33.0" onMouseClicked="#handleExit" pickOnBounds="true" preserveRatio="true" styleClass="cross">
               <image>
                  <Image url="@img/close.png" />
               </image>
               <viewport>
                  <Rectangle2D />
               </viewport>
            </ImageView>
            <ImageView fitHeight="31.0" fitWidth="33.0" onMouseClicked="#handleMinimizar" pickOnBounds="true" preserveRatio="true" styleClass="minimize" />
            <HBox prefHeight="30.0" prefWidth="492.0" />
            <ImageView fx:id="ivDelete" fitHeight="31.0" fitWidth="30.0" onMouseClicked="#deleteModeIv" pickOnBounds="true" preserveRatio="true" styleClass="delete">
               <image>
                  <Image url="@img/delete.png" />
               </image></ImageView>
         </items></ToolBar>
   </top>
</BorderPane>
